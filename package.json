	"devDependencies": {
		"@swc-node/jest": "^1.5.2",
		"@testing-library/jest-dom": "^5.16.5",
		"@testing-library/react": "^13.4.0",
		"@types/jest": "^29.0.0",
		"@types/node": "^16.11.6",
		"@types/react": "18.0.14",
		"@types/react-dom": "18.0.5",
		"@typescript-eslint/eslint-plugin": "5.29.0",
		"@typescript-eslint/parser": "5.29.0",
