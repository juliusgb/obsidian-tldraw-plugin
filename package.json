{
	"name": "obsidian-tldraw-plugin",
	"private": true,
	"version": "0.0.4",
	"description": "An Obsidian plugin to edit and view Tldraw drawings.",
	"author": "Julius Gamanyi",
	"repository": {
		"type": "git",
		"url": "https://github.com/juliusgb/obsidian-tldraw-plugin.git"
	},
